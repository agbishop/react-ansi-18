{
  "name": "@agbishop/react-ansi-18",
  "version": "3.0.2",
  "description": "",
  "author": "agbishop",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agbishop/react-ansi-18.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "moduleFileExtensions": [
    "ts",
    "js"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "storybook": "start- -p 6006",
    "build-storybook": "build-storybook"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "bugs": {
    "url": "https://github.com/agbishop/react-ansi-18/issues"
  },
  "homepage": "https://github.com/agbishop/react-ansi-18e#readme",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.6",
    "@tanstack/react-virtual": "^3.0.0-beta.39",
    "anser": "^2.1.1",
    "escape-carriage": "^1.3.0",
    "immer": "^9.0.19",
    "tss-react": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "files": [
    "dist",
    "es"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-viewport": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.1.21",
    "@storybook/testing-library": "^0.0.13",
    "@tanstack/react-virtual": "^3.0.0-beta.41",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "babel-loader": "^9.1.2",
    "gh-pages": "^5.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4",
    "yorkie": "^2.0.0"
  }
}
